<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mrj.dm.domain">
  <class name="CapitalFlow" table="CAPITAL_FLOW">
    <id column="CAPITAL_FLOW_UUID" name="capitalFlowUuid" type="java.lang.String">
      <generator class="uuid"/>
    </id>
    <property column="USER_UUID" name="userUuid" type="java.lang.String"/>
    <property column="STO_NAME" name="stoName" type="java.lang.String"/>
    <property column="OPERATION_TYPE" name="operationType" type="java.lang.String"/>
    <property column="ACTION_TIME" name="actionTime" type="java.util.Date"/>
    <property column="CHARGE_PRICE" name="chargePrice" precision="12"/>
    <property column="CHARGE_AMOUNT" name="chargeAmount" type="int"/>
    <property column="FEE" name="fee" precision="12"/>
    <property column="LEFT_FEE" name="leftFee" precision="12"/>
    <property column="BROKERAGE" name="brokerage" precision="12"/>
    <property column="STO_CODE" name="stoCode" type="java.lang.String"/>
    <property column="INVEST_RESULT_UUID" name="investResultUuid" type="java.lang.String"/>
    
  </class>
</hibernate-mapping>
