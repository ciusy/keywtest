<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mrj.dm.domain">
	<class name="CapitalFlow" table="CAPITAL_FLOW">
		<id name="capitalFlowUuid" column="CAPITAL_FLOW_UUID" type="java.lang.String">
			<generator class="uuid" />
		</id>
		<property name="userUuid" type="java.lang.String" column="USER_UUID" />
		<property name="operationType" type="java.lang.String" column="OPERATION_TYPE" />
		
	</class>
</hibernate-mapping>